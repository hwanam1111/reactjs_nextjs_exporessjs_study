\n        draft.me.Posts.unshift({\n          id: action.data\n        });\n        break;\n\n      case REMOVE_POST_OF_ME:\n        {\n          draft.me.Posts = draft.me.Posts.filter(function (v) {\n            return v.id !== action.data;\n          });\n          break;\n        }\n\n      default:\n        break;\n    }\n  });\n};\n\nexport default userReduce;","map":{"version":3,"sources":["/Users/leejunkim/Desktop/development_study/fullstack-nodebird/prepare/front/reducers/user.js"],"names":["produce","initialState","loginLoading","loginComplete","loginError","logoutLoading","logoutComplete","logoutError","signupLoading","signupComplete","signupError","changeNicknameLoading","changeNicknameComplete","changeNicknameError","followLoading","followComplete","followError","unfollowLoading","unfollowComplete","unfollowError","me","signUpDate","loginDate","LOG_IN_REQUEST","LOG_IN_SUCCESS","LOG_IN_FAILURE","LOG_OUT_REQUEST","LOG_OUT_SUCCESS","LOG_OUT_FAILURE","SIGN_UP_REQUEST","SIGN_UP_SUCCESS","SIGN_UP_FAILURE","CHANGE_NICKNAME_REQUEST","CHANGE_NICKNAME_SUCCESS","CHANGE_NICKNAME_FAILURE","FOLLOW_REQUEST","FOLLOW_SUCCESS","FOLLOW_FAILURE","UNFOLLOW_REQUEST","UNFOLLOW_SUCCESS","UNFOLLOW_FAILURE","ADD_POST_TO_ME","REMOVE_POST_OF_ME","loginRequestAction","data","type","logoutRequestAction","signupRequestAction","changeNicknameAction","followAction","unfollowAction","userReduce","state","action","draft","dummyUserData","error","Followings","push","id","filter","v","Posts","unshift"],"mappings":"AAAA,OAAOA,OAAP,MAAoB,OAApB;AAEA,OAAO,IAAMC,YAAY,GAAG;AAC1BC,EAAAA,YAAY,EAAE,KADY;AAE1BC,EAAAA,aAAa,EAAE,KAFW;AAG1BC,EAAAA,UAAU,EAAE,IAHc;AAI1BC,EAAAA,aAAa,EAAE,KAJW;AAK1BC,EAAAA,cAAc,EAAE,KALU;AAM1BC,EAAAA,WAAW,EAAE,IANa;AAO1BC,EAAAA,aAAa,EAAE,KAPW;AAQ1BC,EAAAA,cAAc,EAAE,KARU;AAS1BC,EAAAA,WAAW,EAAE,IATa;AAU1BC,EAAAA,qBAAqB,EAAE,KAVG;AAW1BC,EAAAA,sBAAsB,EAAE,KAXE;AAY1BC,EAAAA,mBAAmB,EAAE,IAZK;AAa1BC,EAAAA,aAAa,EAAE,KAbW;AAc1BC,EAAAA,cAAc,EAAE,KAdU;AAe1BC,EAAAA,WAAW,EAAE,IAfa;AAgB1